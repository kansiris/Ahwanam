                </ul>
            </div>
            <div class="w-100 text-center">
                <p class="mb-0">Copy Right 2018, All rights reserved.</p>
            </div>
        </div>
    </footer>


    <script type="text/javascript" src="~/newdesign/js/jquery-3.3.1.min.js"></script>
    <script type="text/javascript" src="~/newdesign/js/bootstrap.js"></script>

    <!------ Date Picker ---------->
    <script type="text/javascript" src="~/newdesign/js/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/newdesign/js/datepicker.js"></script>

    <!------ Owl Thumb Slider ---------->
    <script type="text/javascript" src="~/newdesign/js/owl.carousel.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

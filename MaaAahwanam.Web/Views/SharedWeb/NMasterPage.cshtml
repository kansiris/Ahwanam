    <script>
        $(function () {
            var availableTags = '';
            $.ajax({
                type: "GET",
                url: '/NHomePage/AutoCompleteCountry',
                success: function (data) {
                    availableTags = data.split(',');
                    $("#tags,#tags1").autocomplete({
                        source: availableTags
                    });
                }
            });
            $("#tags, #tags1").autocomplete({
                source: availableTags
            });
        });
    </script>
    <script>
        /*------- Common Search Block -------------*/

        /*------ Index Search Block --------*/

        $(".commonSearch input[type='text']").on('input', function () {
            if ($(this).val().length >= 2) {
                $(".commonSearch + .commonSearchResults > div").css("display", "block");
            } else {
                $(".commonSearch + .commonSearchResults > div").css("display", "none");
            }

        <script>
            $("input").each(function () {
                var $this = $(this);
                if ($(this).val().trim() == "") {
                    $(this).addClass("input-empty");
                } else {
                    $(this).removeClass("input-empty");
                }
                $this.on("change", function () {
                    if ($(this).val().trim() == "") {
                        $(this).addClass("input-empty");
                    } else {
                        $(this).removeClass("input-empty");
                    }
                });
            });
        </script>


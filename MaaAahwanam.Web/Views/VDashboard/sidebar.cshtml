    <div id="addservices" style="height:150px">
        <div id="addcategory" style="height:60px">
            <select class="form-control" id="serviceselection1" name="serviceselection1" style="height:40px">
                <option disabled selected>Select Service Type</option>
                <option value="Venue">Venue</option>
                <option value="Catering">Catering</option>
                <option value="Photography">Photography</option>
                <option value="Decorator">Decorator</option>
                <option value="Other">Others</option>
            </select>
        </div>
        @*<input type="hidden" id="serviceselection" name="serviceselection" value="@ViewBag.category" />*@
        <div class="addsubcategory">
            <select class="form-control" id="subcategory" name="subcategory" style="display:none ;height:40px"></select>
        </div>
        <div>
            <button type="button" onclick="addservicenow()" class="btn btn-1 addbutton" style="border-radius:1px;width:230px;color:#fff;background-color:red; font-size:14px;">
                submit
            </button>
        </div>
    </div>
    <div id="addservicesbutton" style="height:150px">
        <button type="button" onclick="addservice()" class="btn btn-1 " style="border-radius:1px;width:230px;color:#fff;background-color:red; font-size:14px;">

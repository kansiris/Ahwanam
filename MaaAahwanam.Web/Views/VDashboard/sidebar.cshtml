<style>
    .h6-in {
        cursor: pointer;
    }
</style>
<div class="col-md-2-2" id="menusidebar">
    <br />
    <h6 class="h6-in" onclick="location.href = '/VDashboard'">Dashboard	</h6>
    <h6 class="h6-in" onclick="location.href = '/VDashboard?c=orders'">Orders</h6>
    <h6 class="h6-in" onclick="location.href = '/ManageUser'">Customers/clients</h6>
    <h6 class="h6-in" onclick="location.href = '/ManageVendor'">Vendors</h6>
    <h6 class="h6-in">Resellers/partners</h6>
    <h6 class="h6-in">Add Availability</h6>
    <div role="tablist" class="add_bottom_45 accordion_2" id="allservices">
        <div class="card">
            <div class="card-header" role="tab">
                <h5 class="mb-0">
                    <a data-toggle="collapse" href="#collapseOne_allservices" aria-expanded="false" style="text-transform:uppercase">
                        <i class="indicator ti-minus"></i>Services
                    </a>
                </h5>
            </div>

            <div id="collapseOne_allservices" class="collapse show" role="tabpanel" data-parent="#allservices">
                <div class="card-body">
                    @foreach (var item in ViewBag.venues)
                    {
                        <h6 class="h6-in" onclick="location.href = '/VDashboard/Index?c=first&&vsid=' + @item.Id">@item.name - @item.VenueType</h6>
                    }
                    @foreach (var item in ViewBag.catering)
                    {
                        <h6 class="h6-in" onclick="location.href = '/VDashboard/Index?c=first&&vsid=' + @item.Id">@item.CuisineType</h6>
                    }
                    @foreach (var item in ViewBag.photography)
                    {
                        <h6 class="h6-in" onclick="location.href = '/VDashboard/Index?c=first&&vsid=' + @item.Id">@item.PhotographyType</h6>
                    }
                    @foreach (var item in ViewBag.decorators)
                    {
                        <h6 class="h6-in" onclick="location.href = '/VDashboard/Index?c=first&&vsid=' + @item.Id">@item.DecorationType</h6>
                    }
                    @foreach (var item in ViewBag.others)
                    {
                        <h6 class="h6-in" onclick="location.href = '/VDashboard/Index?c=first&&vsid=' + @item.Id">@item.type</h6>
                    }
                </div>
            </div>
        </div>
    </div>

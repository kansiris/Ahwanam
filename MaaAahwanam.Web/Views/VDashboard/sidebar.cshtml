



<div class="col-md-2-2">
    <br />
    <h6 class="h6-in">Dashboard	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
   <h6 class="h6-in">Manage Orders		<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
   <h6 class="h6-in">Services	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
    @*<h6 class="h6-in">untitled	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>*@
    @foreach (var item in ViewBag.venues)
    {
        <h6 class="h6-in">@item.VenueType	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
    }
@foreach (var item in ViewBag.catering)
{
    <h6 class="h6-in">@item.CuisineType	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
}
    @foreach (var item in ViewBag.photography)
    {
        <h6 class="h6-in">@item.PhotographyType	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
    }
    @foreach (var item in ViewBag.decorators)
    {
        <h6 class="h6-in">@item.DecorationType	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
    }
    @foreach (var item in ViewBag.others)
    {
        <h6 class="h6-in">@item.type	<a href="#" class="next"><img src="data:image/svg+xml;utf8;base64," /></a></h6>
    }
    <div id="addservices" style="height:150px">
        <div id="addcategory" style="height:50px">
            <select class="form-control" id="serviceselection1" name="serviceselection1">
                <option disabled selected>Select Service Type</option>
                <option value="Venue">Venue</option>
                <option value="Catering">Catering</option>
                <option value="Photography">Photography</option>
                <option value="Decorator">Decorator</option>
                <option value="Other">Others</option>
            </select>
        </div>
        @*<input type="hidden" id="serviceselection" name="serviceselection" value="@ViewBag.category" />*@
        <div class="addsubcategory">
            <select class="form-control" id="subcategory" name="subcategory" style="display:none"></select>
        </div>

    </div>
    <button type="button" onclick="addservice()" class="btn btn-1" style="border-radius:1px;width:230px;color:#fff;background-color:red; font-size:14px;">
        Add Service
    </button>

</div>
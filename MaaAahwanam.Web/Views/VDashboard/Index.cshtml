                    alert('Invalid File Type');
                }
                else {
                    var data = new FormData();
                    var files = $("#profileimage").get(0).files;
                    //alert(files);
                    if (files.length > 0) {
                        data.append("helpSectionImages", files[0]);
                    }
                    alert($('#Item1_EmailId').val());
                    $.ajax({
                        url: '/VDashboard/UploadProfilePic?email=' + $('#Item1_EmailId').val(),
                        type: "POST",
                        processData: false,

                            if (item.name != null && item.name != "")
                            {
                                <a style="text-transform: uppercase;font-size:16px" class="tablink" href="#@(++services)"><b>@item.name</b></a>
                            }
                            else
                            {
                                <a style="text-transform: uppercase;font-size:16px" class="tablink" href="#@(++services)"><b>@item.VenueType</b></a>
                            }
                        }
                    </div>
                </div>
                <br><br>
                @foreach (var item in ViewBag.particularVenue)
                {
                    <div class="container" id="@(++servicesdiv)">

    <script>
        function planning() {
            var location = $('#tags').val();
            var servicetype = $('#selectedservice').val();
            var occasiondate = $('#datetimepicker').val();
            if (location == '') { location = 'Hyderabad'; }
            if (servicetype == '') { alert("Select Service Type"); }
            if (occasiondate == '') { alert("Select Occasion Date"); }
            //alert("location=" + location + ",servicetype=" + servicetype + ",occasiondate=" + occasiondate + "");
            if (servicetype != '' && occasiondate != '') {
                window.location.href = '/NQuestionNaire?location=' + location + '&&servicetype=' + servicetype + '&&occasiondate=' + occasiondate;

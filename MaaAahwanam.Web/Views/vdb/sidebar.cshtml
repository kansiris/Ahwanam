    <!--Accordion wrapper-->
    <div class="row accordion md-accordion" id="accordionEx" role="tablist" aria-multiselectable="true" style="padding:0;width:115%">

        <!-- Accordion card -->
        <div class="card">

            <!-- Card header -->
            <div class="card-header" role="tab" id="headingOne1">
                <a data-toggle="collapse" data-parent="#accordionEx" href="#collapseOne1" aria-expanded="true" aria-controls="collapseOne1">
                    @*<h5 class="mb-0">
                        Services <i class="fa fa-angle-down rotate-icon"></i>
                    </h5>*@
                    <div>Services <i class="fa fa-angle-down rotate-icon" style="float:right"></i></div>
                </a>
            </div>

            <!-- Card body -->
            <div id="collapseOne1" class="collapse show" role="tabpanel" aria-labelledby="headingOne1" data-parent="#accordionEx">
                <div class="card-body">
                    <div class="panel-body smooth-scroll list-unstyled">
                        @foreach (var item in ViewBag.venues)
                        {
                            if (item.VenueType == "" || item.VenueType == null)
                            {
                                <div><a class="subcatclose" value="@item.Id,Venue" style="color: red;font-size: 12px;">X</a> <a href="/vdb/Index?c=first&&vid=@item.Id">Venue</a></div>
                            }
                            else
                            {
                                <div><a class="subcatclose" value="@item.Id,@item.name,@item.VenueType,Venue" style="color: red;font-size: 12px;">X</a><a href="/vdb/Index?c=first&&vid=@item.Id"> @item.name - @item.VenueType</a></div>
                            }
                        }
                        @foreach (var item in ViewBag.catering)
                        {
                            <div><a class="subcatclose" value="@item.Id,,@item.CuisineType,Catering" style="color: red;font-size: 12px;">X</a> <a href="/VDashboard/Index?c=first&&vsid=@item.Id">@item.CuisineType</a></div>
                        }
                        @foreach (var item in ViewBag.photography)
                        {
                            <div>  <a class="subcatclose" value="@item.Id,,@item.PhotographyType,Photography" style="color: red;font-size: 12px;">X</a> <a href="/VDashboard/Index?c=first&&vsid=@item.Id">@item.PhotographyType</a></div>
                        }
                        @foreach (var item in ViewBag.decorators)
                        {
                            <div><a class="subcatclose" value="@item.Id,,@item.DecorationType,Decorator" style="color: red;font-size: 12px;">X</a> <a href="/VDashboard/Index?c=first&&vsid=@item.Id">@item.DecorationType</a></div>
                        }
                        @foreach (var item in ViewBag.others)
                        {
                            <div>   <a class="subcatclose" value="@item.Id,,@item.type,Other" style="color: red;font-size: 12px;">X</a> <a href="/VDashboard/Index?c=first&&vsid=@item.Id">@item.type</a></div>
                        }
                    </div>
                </div>
            </div>

        </div>
        <!-- Accordion card -->
        <!-- Accordion card -->
        <div class="card">

            <!-- Card header -->
            <div class="card-header" role="tab" id="headingTwo2">
                <a class="collapsed" data-toggle="collapse" data-parent="#accordionEx" href="#collapseTwo2" aria-expanded="false" aria-controls="collapseTwo2">
                    @*<h5 class="mb-0">
                        Resellers/Partners <i class="fa fa-angle-down rotate-icon"></i>
                    </h5>*@
                    <div>Resellers/Partners <i class="fa fa-angle-down rotate-icon" style="float:right"></i></div>
                </a>
            </div>

            <!-- Card body -->
            <div id="collapseTwo2" class="collapse" role="tabpanel" aria-labelledby="headingTwo2" data-parent="#accordionEx">
                <div class="card-body">
                    <div class="panel-body ">
                        <div class="scrollbar scrollbar-primary">
                            <div class="force-overflow">
                                @if (ViewBag.resellername != null)
                                {
                                    foreach (var item in ViewBag.resellername)
                                    {

                                        <div><a class="subcatclose" value="@item.PartnerID,@item.PartnerName,@item.VendorType,Venue" style="color: red;font-size: 12px;">X</a><a href="/ReSellerManagement/Index?partid=@item.PartnerID&&vid=@item.VendorId"> @item.PartnerName</a></div>
                                    }
                                }
                                </div>
                            </div>
                        </div>
                </div>
            </div>

        </div>
        <!-- Accordion card -->
    </div>
    <!-- Accordion wrapper -->

</div>


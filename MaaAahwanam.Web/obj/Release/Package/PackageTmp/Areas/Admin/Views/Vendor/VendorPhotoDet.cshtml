    function DeleteProduct(pid) {
        if (confirm("Do you want to delete Image: " + pid)) {
            var data = { 'ImgId': pid }
            $.post('/Vendor/DeleteImg', data,
            function (data) {
                if (data == true) {
                    var qrStr = window.location.search;
                    qrStr = qrStr.split("=")[1];
                    location.href = "/Admin/Vendor/VendorPhotoDet?id=" + qrStr;
                    viewbag.I1 = "active";
                }
                else
                    alert("Could not delete");
            });
        }
    }


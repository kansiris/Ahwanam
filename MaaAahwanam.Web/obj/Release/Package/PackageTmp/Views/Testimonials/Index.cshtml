
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/SharedWeb/ServicesMaster.cshtml";
}

@section Navigation
{
<div class="breads">
    <div class="breads_inner">
        <div class="pageWidth">
            <div class="title">
                <h3>Testimonials</h3>
            </div>
            <div class="breadcrumbs">
                <ul>
                    <li><a href="../index/index">Home</a></li>
                    <li><i class="breads_arrow"></i></li>
                    <li>Testimonials</li>
                </ul>
            </div>
        </div>
    </div>
</div>
}
<article>
    <div class="about_categories">
        <select>
            <option>Beautician</option>
            <option>Photographers</option>
            <option>Cake Vendors</option>
            <option>Caterer Decorators</option>
            <option>Designer Photographers</option>
            <option>Entertainment</option>
            <option>Fireworks</option>
            <option>Godi wale</option>
        </select>
    </div>
    <div class="about_content">
        <div class="testimonials_inner">
            @{Html.RenderAction("TestimonialsBindingLayout", "Index", new { view = "2" });}
        </div>


    </div>
</article>

@section advertisement
{
    <div class="ad">
        <div class="pageWidth">
            <div class="ad_728"><img src="../CSSWeb/images/ad_728.jpg" alt="advertise"></div>
        </div>
    </div>
}